{
  "singleQuote": false,
  "printWidth": 200,
  "endOfLine": "lf",
  "tabWidth": 4,
  "useTabs": true,
  "trailingComma": "none",
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": ["*.yaml", "*.yml", "*.md", "*.json"],
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ]
}
