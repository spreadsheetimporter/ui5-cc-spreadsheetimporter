{
  "name": "ui5-spreadsheetimporter",
  "version": "0.0.0",
  "description": "Modules Spreadsheet Upload for Fiori Element Apps.",
  "main": "Component.js",
  "private": true,
  "author": "Marian Zeis",
  "homepage": "https://spreadsheet-importer.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/spreadsheetimporter/ui5-cc-spreadsheetimporter.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm-run-all --parallel start:server start:v4fe",
    "start:v2fe": "npm run start --workspace=ordersv2fe136",
    "start:v4fe": "npm run start --workspace=ordersv4fe136",
    "start:anyupload": "npm run start --workspace=anyupload",
    "start:v4freestyle": "npm run start --workspace=ordersv4freestyle136",
    "test:v4fe": "npm run test --workspace=ui5-cc-spreadsheetimporter-sample -- -- ordersv4fe 136",
    "test:opa5:v4fe": "npm run ui5-test-runner --workspace=ui5-cc-spreadsheetimporter-sample --url http://localhost:8080/test/integration/opaTests.qunit.html",
    "test:v2fe": "npm run test --workspace=ui5-cc-spreadsheetimporter-sample -- -- ordersv2fe 136",
    "start:server": "npm run watch:server --workspace=ui5-cc-spreadsheetimporter-sample",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint -e",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "build": "npm run build --workspace=ui5-cc-spreadsheetimporter",
    "replaceString": "node ./dev/replace-string.js",
    "build:mta": "npm run build:mta --workspace=ui5-cc-spreadsheetimporter",
    "deploy:cf": "npm run deploy:cf --workspace=ui5-cc-spreadsheetimporter",
    "copyTestApps": "node ./dev/copy-example-apps.js create && npm install --verbose",
    "deleteAllNodeModules": "node dev/util.js deleteNodeModules .",
    "deleteTestApps": "node ./dev/copy-example-apps.js  delete",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml -y",
    "runDocs": "docker run --rm -it -p 8000:8000 -v ${PWD}:/docs squidfunk/mkdocs-material",
    "coypChangelogToDoc": "node ./dev/copy-changelog-to-doc.js",
    "updateAllDependencies": "npm update --workspaces",
    "lint:ui5": "npm run lint:ui5 --workspace=ui5-cc-spreadsheetimporter",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.27.5",
    "@babel/preset-env": "^7.27.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@sap/ux-ui5-tooling": "1.18.0",
    "@sapui5/types": "1.136.2",
    "@types/node": "24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@ui5/cli": "4.0.18",
    "@ui5/project": "4.0.4",
    "@ui5/ts-interface-generator": "0.10.0",
    "babel-preset-transform-ui5": "^7.7.1",
    "bluebird": "3.7.2",
    "husky": "^9.1.7",
    "chokidar": "^4.0.3",
    "eslint": "9.29.0",
    "js-yaml": "4.1.0",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.3",
    "replace-in-file": "^8.3.0",
    "shelljs": "^0.10.0",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yaml,yml,css,html,xml,properties}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "examples",
    "examples/packages/*",
    "packages/*",
    "examples/packages/server/app/*"
  ]
}
