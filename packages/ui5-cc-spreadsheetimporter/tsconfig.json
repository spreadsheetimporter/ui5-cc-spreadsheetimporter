{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "declaration": true,
    "rootDir": "./",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "./node_modules/@sapui5/ts-types-esm"],
    "paths": {
      "cc/spreadsheetimporter/XXXnamespaceXXX/*": ["packages/ui5-cc-spreadsheetimporter/src/*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", "dist"]
}
